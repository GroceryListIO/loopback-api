{
  "name": "Item",
  "plural": "items",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuidv4",
      "description": "Item ID"
    },
    "listId": {
      "type": "string",
      "description": "List ID",
      "required": true
    },
    "userId": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "quantity": {
      "type": "number"
    },
    "inventory": {
      "type": "number"
    },
    "aisle": {
      "type": "string"
    },
    "price": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "list": {
      "type": "belongsTo",
      "model": "List",
      "foreignKey": "id"
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "id"
    }
  },
  "acls": [],
  "methods": {}
}
