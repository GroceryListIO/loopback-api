{
  "name": "List",
  "plural": "lists",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "injectOptionsFromRemoteContext": true,
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuidv4",
      "description": "List ID"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "userId": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "items": {
      "type": "hasMany",
      "model": "Item",
      "foreignKey": "listId"
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "id"
    }
  },
  "acls": [],
  "methods": []
}
